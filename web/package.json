    "react-is": "^19.1.1",
    "react-loading-skeleton": "^3.5.0",
    "react-markdown": "^10.1.0",
    "react-qr-code": "^2.0.18",
    "react-resize-detector": "^12.1.0",
    "react-router": "^6.30.1",
    "react-router-dom": "^6.30.1",
    "react-tracked": "^2.0.1",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",

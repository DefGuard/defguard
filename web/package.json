    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.2.0",
    "@types/qs": "^6.14.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react-swc": "^3.11.0",

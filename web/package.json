    "qrcode": "^1.5.4",
    "radash": "^12.1.0",
    "react": "^18.2.0",
    "react-click-away-listener": "^2.2.4",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.0",
    "react-idle-timer": "^5.7.2",
    "react-is": "^18.2.0",
    "react-loading-skeleton": "^3.5.0",
    "react-markdown": "^9.0.1",
    "react-qr-code": "^2.0.15",
    "react-resize-detector": "^12.0.1",

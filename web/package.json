{
  "name": "web",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm run vite\" \"pnpm run typesafe-i18n\"",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "generate-translation-types": "typesafe-i18n --no-watch",
    "lint": "eslint src/ && prettier --check 'src/**/*.{ts,tsx,scss}' && tsc --noEmit",
    "fix": "prettier -w 'src/**/*.{ts,tsx,scss}' && eslint --fix src/",
    "typesafe-i18n": "typesafe-i18n",
    "vite": "vite",
    "eslint": "eslint",
    "prettier": "prettier"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react-native"
      ]
    },
    "ignoredBuiltDependencies": [
      "@swc/core",
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "@swc/core"
    ]
  },

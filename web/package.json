    "hex-rgb": "^5.0.0",
    "html-react-parser": "^4.0.0",
    "itertools": "^2.1.1",
    "lodash-es": "^4.17.21",
    "numbro": "^2.3.6",
    "qrcode": "^1.5.3",
    "radash": "^11.0.0",
    "react": "^18.2.0",
    "react-click-away-listener": "^2.2.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
